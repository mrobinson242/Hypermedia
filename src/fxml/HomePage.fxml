<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.Pane?>

<fx:root prefHeight="600.0" type="Pane" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar fx:id="_menuBar" prefHeight="30.0" prefWidth="1200.0">
         <menus>
            <Menu fx:id="_fileMenu" mnemonicParsing="false" text="File">
               <items>
                  <MenuItem fx:id="_importPrimaryVideoButton" mnemonicParsing="false" text="Import Primary Video" />
                  <MenuItem fx:id="_importSecondaryVideoButton" mnemonicParsing="false" text="Import Secondary Video" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit" />
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <Pane layoutY="30.0" prefHeight="50.0" prefWidth="1200.0">
         <children>
            <ToggleButton fx:id="_videoToolButton" mnemonicParsing="false" prefHeight="50.0" prefWidth="90.0" text="Hyperlink Video Tool" textAlignment="CENTER" wrapText="true" />
            <ToggleButton fx:id="_videoPlayerButton" layoutX="90.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="90.0" text="Interactive Video Player" textAlignment="CENTER" wrapText="true" />
         </children>
      </Pane>
      <Pane fx:id="_contentPane" layoutY="80.0" prefHeight="520.0" prefWidth="1200.0" styleClass="contentPane" />
   </children>
</fx:root>
